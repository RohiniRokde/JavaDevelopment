<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.3.xsd">


<bean id="address" class="com.byType.Address" autowire-candidate="false">
<property name="areaname" value="nigdi"></property>
<property name="cityname" value="pune"></property>

</bean>

<bean id="address2" class="com.byType.Address">
<property name="areaname" value="akurdi"></property>
<property name="cityname" value="pune"></property>

</bean>
<bean id="stu" class="com.byType.Student" autowire="byType">
<property name="rollno" value="101"></property>
<property name="name" value="yug"></property>
</bean>



</beans>
