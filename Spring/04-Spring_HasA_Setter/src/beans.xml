<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.3.xsd">


  <bean id= "stu" class="com.HasA.Student">
  
  <property name="rollno" value="101"></property>
  <property name="name" value="Monu"></property>
  <property name="add" ref="adr"></property>
 
  
  </bean>
  
  <bean id="adr" class="com.HasA.Address">
    <property name="pincode" value="411017"></property>
     <property name="area" value="Ravet"></property>
      <property name="city" value="pune"></property>
    
  </bean>
</beans>
